{
  "name": "webpack3-react-base",
  "version": "1.0.0",
  "main": "index.js",
  "author": "David Burles",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --progress"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "butternut-webpack-plugin": "^0.0.6",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "prop-types": "^15.5.10",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "recompose": "^0.23.5"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      [
        "transform-object-rest-spread"
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "rules": {
      "func-names": "off",
      "react/jsx-filename-extension": "off"
    }
  }
}
